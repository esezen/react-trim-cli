<import component='renderRoutes' from='react-router-config' />
<import component='Link' from='react-router-dom' />
<import component='Menu' from='semantic-ui-react' />
<import default='isRoute' from='scripts/routes' />

<load from='styles/semantic/semantic.min.css' />
<load from='styles/Template.css' />

<React.Fragment @stateless>
    <Menu inverted color='red' fixed='top'>
        <Menu.Item as={ Link } to='/' active={ isRoute('/') }>
        Basic Example
        </Menu.Item>

        <Menu.Item as={ Link } to='/users' active={ isRoute('/users') }>
        Users Example
        </Menu.Item>

        <Menu.Item as={ Link } to='/feed' active={ isRoute('/feed') }>
        News Feed Example
        </Menu.Item>
    </Menu>
    { renderRoutes(props.route.routes) }
</React.Fragment>
